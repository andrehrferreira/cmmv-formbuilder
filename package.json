{
    "name": "@cmmv/formbuilder",
    "version": "0.0.2",
    "main": "Form and listing screen generator in Vue3 + Tailwindcss for CMMV",
    "scripts": {
        "prebuild": "pnpm run clean",
        "clean": "rimraf dist",
        "dev": "NODE_ENV=dev concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
        "dev:server": "NODE_ENV=dev nodemon",
        "dev:client": "NODE_ENV=dev vite",
        "test": "vitest",
        "build": "tsc -p tsconfig.server.json && mv dist/src dist/types",
        "build:module": "vite build --mode module",
        "build:dev": "pnpm run clean && pnpm run type-check && vite build --mode dev",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "release": "node release.cjs",
        "lint": "pnpm run lint:spec",
        "lint:fix": "pnpm run lint:spec -- --fix",
        "lint:spec": "eslint 'tests/**/**.spec.ts' --no-ignore",
        "type-check": "vue-tsc --build",
        "prepare": "husky"
    },
    "type": "module",
    "engines": {
        "node": ">= 18.18.0 || >= 20.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/andrehrferreira/cmmv-formbuilder.git"
    },
    "keywords": [
        "cmmv",
        "form",
        "pages",
        "builder",
        "components",
        "view"
    ],
    "author": "Andre Ferreira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andrehrferreira/cmmv-formbuilder/issues"
    },
    "homepage": "https://github.com/andrehrferreira/cmmv-formbuilder#readme",
    "description": "CMMV (@formbuilder)",
    "changelog": {
        "labels": {
            "type: feature :tada:": "Features",
            "type: bug :sob:": "Bug fixes",
            "type: enhancement :wolf:": "Enhancements",
            "type: docs :page_facing_up:": "Docs",
            "type: code style": "Code style tweaks",
            "dependencies": "Dependencies"
        }
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --ignore-path ./.prettierignore --write"
        ]
    },
    "devDependencies": {
        "@cmmv/http": "^0.5.11",
        "@cmmv/protobuf": "^0.5.11",
        "@cmmv/repository": "^0.5.11",
        "@cmmv/view": "^0.5.11",
        "@cmmv/vite": "^0.5.11",
        "@cmmv/ws": "^0.5.11",
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-angular": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "@swc-node/core": "^1.13.3",
        "@swc-node/register": "^1.10.9",
        "@swc/core": "^1.10.0",
        "@swc/helpers": "^0.5.15",
        "@tsconfig/node22": "^22.0.0",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.10.1",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.20",
        "chalk": "^5.3.0",
        "concurrently": "^9.1.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "enquirer": "^2.4.1",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "execa": "^9.5.2",
        "husky": "^9.1.7",
        "jsdom": "^25.0.1",
        "jsdom-global": "^3.0.2",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.7",
        "postcss": "^8.4.49",
        "postcss-nesting": "^13.0.1",
        "prettier": "^3.4.2",
        "sass": "^1.82.0",
        "semver": "^7.6.3",
        "sqlite3": "^5.1.7",
        "tailwindcss": "^3.4.16",
        "terser": "^5.36.0",
        "typeorm": "^0.3.20",
        "typescript": "^5.7.2",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0",
        "vite-tsconfig-paths": "^5.1.3",
        "vitest": "^2.1.8",
        "vue": "^3.5.13",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@cmmv/core": "^0.5.11",
        "@cmmv/ui": "^0.0.8",
        "@vueform/plugin-mask": "^1.0.7",
        "@vueform/vueform": "^1.11.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "datatables.net-dt": "^2.1.8",
        "datatables.net-select": "^2.1.0",
        "datatables.net-select-dt": "^2.1.0",
        "datatables.net-vue3": "^3.0.2",
        "fast-json-stringify": "^6.0.0",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^6.0.1",
        "sass-embedded": "^1.82.0",
        "vue-i18n": "^10.0.5",
        "vue-router": "^4.5.0"
    }
}
